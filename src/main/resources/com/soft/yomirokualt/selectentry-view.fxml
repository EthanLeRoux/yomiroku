<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soft.yomirokualt.controller.SelectEntryController">
   <children>
      <Label layoutX="200.0" layoutY="30.0" text="Select a entry Type">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnVideoEntry" layoutX="188.0" layoutY="168.0" mnemonicParsing="false" onAction="#switchToVideo" styleClass="app_button" text="Video Entry" />
      <Button fx:id="btnBookEntry" layoutX="189.0" layoutY="119.0" mnemonicParsing="false" onAction="#switchToBook" styleClass="app_button" text="Book Entry" />
   </children>
</AnchorPane>
